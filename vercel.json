{
    "version": 2,
    "builds": [
      {
        "src": "*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": {
          "Content-Security-Policy": "default-src 'self' 'unsafe-inline'"
        }
      }
    ]
  }